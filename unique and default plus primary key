CREATE TABLE IF NOT EXISTS countries (
COUNTRY_ID varchar(2) NOT NULL UNIQUE DEFAULT '',
COUNTRY_NAME varchar(40) DEFAULT NULL,
REGION_ID decimal(10,0) NOT NULL,
PRIMARY KEY (COUNTRY_ID,REGION_ID));

insert into countries values(1, 'Pakistan', 102);
insert into countries(country_name, region_id) values( 'Germany', 4);
insert into countries(country_name, region_id) values ( 'USA', 10)
